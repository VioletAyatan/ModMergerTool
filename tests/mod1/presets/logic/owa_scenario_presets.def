sub declare_owa_scenario_settings()
{
    PresetDef("Scenario")
    {
        Description("");
        AddField("SELECTOR", "", false, true, false);
		AddField("CutTinyObjects", "", false, true, false);
        IgnoreField("UserData");
        IgnoreField("SkinName");
        Preset("False")
        {
            SetField("SELECTOR", "False");
        }
        Preset("True")
        {
            SetField("SELECTOR", "True");
        }
    }
    PresetDef("CutTinyObjects")
    {
        Description("");
		AddField("CutTinyObjects", "", false, true, false);
        IgnoreField("UserData");
        IgnoreField("SkinName");
        Preset("False")
        {
            SetField("CutTinyObjects", "0");
        }
        Preset("True")
        {
            SetField("CutTinyObjects", "1");
        }
    }
}
	