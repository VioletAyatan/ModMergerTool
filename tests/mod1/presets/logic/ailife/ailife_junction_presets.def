sub declare_junction_params()
{
    PresetDef("Junction")
    {
        Description("");
        
        AddField("m_AIsSearchingRadius", "0");
        AddField("m_RequestedAIsCount", "0");
        AddField("m_AllowOnlyUniqueAIPresets", "0");
        
        Preset("test_junction")
        {
            SetField("m_AIsSearchingRadius", "10");
            SetField("m_RequestedAIsCount", "1");
            SetField("m_AllowOnlyUniqueAIPresets", "0");
        }
        
        Preset("City_Center_Params")
        {
            SetField("m_AIsSearchingRadius", "15");
            SetField("m_RequestedAIsCount", "1");
        }
    }
}