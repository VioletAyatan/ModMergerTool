sub declare_spawn_probability()
{
    PresetDef("Spawn probability")
    {
        AddField("m_SpawnProbability", "1");
        
        Preset("Always")
        {
            SetField("m_SpawnProbability", "1");
        }
        Preset("50/50")
        {
            SetField("m_SpawnProbability", "0.5");
        }
    }
}