sub declare_scenario_ending_policy()
{
    PresetDef("Ending Policy")
    {
        AddField("m_MinExecutionTime", "30.0");
        AddField("m_MaxExecutionTime", "45.0");

        Preset("Endless")
        {
            SetField("m_MinExecutionTime", "-1.0");
            SetField("m_MaxExecutionTime", "-1.0");
        }

        Preset("End After Dialog (Only For Dialog Scenarios)")
        {
            // We dont want to end scenario at same time as dialog ends
            // so continue scenario execution for a little random time
            SetField("m_MinExecutionTime", "3.0");
            SetField("m_MaxExecutionTime", "5.0");
        }
    }
}