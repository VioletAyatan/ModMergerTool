sub declare_embeded_object_requests()
{
    PresetDef("AI Requests")
    {
        AddField("m_RequestObjects", "");

        Preset("EnablePanicFlee")
        {
            SetField("m_RequestObjects", "ai::RequestPanicFleeBehaviorEmbeddedObject,0,(PanicFlee;Enable);");
        }

        Preset("DisablePanicFlee")
        {
            SetField("m_RequestObjects", "ai::RequestPanicFleeBehaviorEmbeddedObject,0,(PanicFlee;Disable);");
        }
    }
}