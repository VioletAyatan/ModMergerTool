sub declare_scenario_type()
{
    PresetDef("Scenario Type")
    {
        AddField("m_IsBackgroundScenario", "0");
        AddField("m_IsInitScenario","0");
        AddField("m_ScenarioBT", "scenario_single_activity_bps", false, true, false);
        
        Preset("Background Non Interactable Scenario")
        {
            SetField("m_IsBackgroundScenario", "1");
            SetField("m_IsInitScenario", "0");
            SetField("m_ScenarioBT", "scenario_endless_runner_bt");
        }
        Preset("Init Scenario")
        {
            SetField("m_IsBackgroundScenario", "0");
            SetField("m_IsInitScenario","1");
        }
    }
}